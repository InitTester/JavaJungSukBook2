public class Ex8_3 {
	// ExceptionEx3
	public static void main(String[] args) {
		// TODO Auto-generated method stub
		int number = 100;
		int result = 0;
		
		for(int i =0; i < 10; i ++) {
			try {
			result = number / (int)(Math.random()*10); // 7번째 라인
			System.out.println(result);
			} catch (Exception e) {
				System.out.println("0"+ " 에러값 : "+ e.getMessage()+ ", 에러 : "+ e.getLocalizedMessage());
			}
		}
	}
}
class Time {
	private int hour;
	private int minute;
	private int second;
	
	Time(int hour, int minute, int second){
		setHour(hour);
		setMinute(minute);
		setSecond(second);		
	}
	
	public int getHour() {	return hour; }
	
	public void setHour(int hour) { 
		if(hour < || hour > 23) return;
		this.minute = minute;
	}
	
}